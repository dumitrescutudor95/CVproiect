    let num3=0;
      function next3(){
        let slider3=document.getElementById("slider3");
        num3++;
        if(num3>=images3.length){num3=0;}
        slider3.src=images3[num3];
      }
      function prev3(){
        let slider3=document.getElementById("slider3");
        num3--;
        if(num3<0){num3=images3.length;}
        slider3.src=images3[num3];
        
      }
    </script>
