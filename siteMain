        let slider=document.getElementById("slider");
        num++;
        if(num>=images.length){num=0;}
        slider.src=images[num];
      }
      function prev(){
        let slider=document.getElementById("slider");
        num--;
        if(num<0){num=images.length;}
        slider.src=images[num];
        
      }
    </script>
