     window.onload = function() {
     let pos = 900; 
    let box = document.getElementById('box');
    let t = setInterval(move, 10);

    function move() {
        if(pos <-300) {
           pos=900;  }
        else {
            pos -= 1;
            box.style.left = pos+'px'; 
        } } };     </script>
   
